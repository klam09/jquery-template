{
    "name": "jquery-template",
    "description": "Jquery template by klam09",
    "version": "1.0.0",
    "author": "klam09",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "webpack --config config/webpack.config.js --env.NODE_ENV=production --progress",
        "build:dev": "npm run build --env.NODE_ENV=development",
        "build:aos": "npm run build --env.APP_ENV=AOS && node scripts/post-build-webview aos",
        "build:ios": "npm run build && node scripts/post-build-webview ios",
        "postbuild": "node scripts/post-build.js",
        "start": "webpack-dev-server --config config/webpack.config.js --env.NODE_ENV=development --progress --colors",
        "eslint": "eslint ./src",
        "eslint:fix": "yarn run eslint -- --fix .",
        "stylelint": "stylelint --ignore-path .eslintignore 'src/**/*.scss' --syntax scss",
        "stylelint:fix": "yarn run stylelint -- --fix"
    },
    "dependencies": {
        "@babel/polyfill": "7.4.3",
        "core-js": "^3.4.0",
        "dropzone": "^5.5.1",
        "jquery": "3.3.1",
        "jquery-validation": "^1.19.1",
        "lottie-web": "5.5.2",
        "moment": "^2.24.0",
        "numeral": "2.0.6",
        "regenerator-runtime": "^0.13.3",
        "select2": "^4.0.12",
        "swiper": "4.2.6",
        "webpack": "4.30.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "autoprefixer": "^8.4.1",
        "babel-loader": "^8.0.5",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^2.1.1",
        "eslint": "^6.6.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-promise": "^4.2.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^4.0.1",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-safe-parser": "^4.0.1",
        "postcss-scss": "^2.0.0",
        "pug": "^3.0.1",
        "pug-html-loader": "^1.1.5",
        "replace-in-file": "^3.4.0",
        "sass-loader": "^7.1.0",
        "shelljs": "^0.8.3",
        "stylelint": "^11.1.1",
        "stylelint-config-airbnb": "^0.0.0",
        "stylelint-order": "^3.1.1",
        "stylelint-scss": "^3.12.1",
        "terser-webpack-plugin": "^1.3.0",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.3.1"
    },
    "browserslist": [
        "Android >= 5",
        "iOS >= 9",
        "last 3 chrome versions",
        "last 3 safari versions",
        "last 3 firefox versions",
        "ie >= 11"
    ],
    "engines": {
        "node": "^10.16.0",
        "npm": "^6.9.0"
    }
}
